{
    "version": 2,
    "builds": [
        {
            "src": "app/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "functions": {
        "app/wsgi.py": {
            "memory": 3008,    
            "maxDuration": 900 
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/wsgi.py"
        }
    ]
}
